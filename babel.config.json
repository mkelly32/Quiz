{
	"presets": [
		["@babel/preset-env", { "targets": { "node": "current" } }],
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"plugins": [
		["@babel/plugin-transform-runtime", { "regenerator": true }],
		"syntax-dynamic-import"
	],
	"env": {
		"test": {
			"plugins": ["dynamic-import-node"]
		}
	}
}
